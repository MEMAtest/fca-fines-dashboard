{
  "name": "fca-fines-scraper",
  "version": "1.0.0",
  "description": "Web scraper for FCA fines data from 2013-2025",
  "main": "scraper.js",
  "scripts": {
    "scrape": "node scraper.js",
    "scrape:year": "node scraper.js --year",
    "test": "node scraper.js --year 2025 --dry-run",
    "test:logic": "node test-scraper.js",
    "test:comprehensive": "node test-comprehensive.js",
    "test:all": "node test-scraper.js && node test-comprehensive.js"
  },
  "keywords": [
    "fca",
    "scraper",
    "puppeteer",
    "supabase"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "puppeteer": "^24.0.0",
    "@supabase/supabase-js": "^2.43.0",
    "dotenv": "^16.4.5"
  },
  "devDependencies": {},
  "engines": {
    "node": ">=16.0.0"
  }
}
